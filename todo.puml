@startuml todo
 class Todo{
    + title: string
    + completed: boolean
    + id: number
 }
 class Add{
    + execute(): TodoList
 }
 class Delete{
    + execute(): TodoList
 }
 class ChangeCompleted{
    + execute(): TodoList
 }

 class Show{
    + all(): void
    + completed(): void
    + uncompleted(): void
 }

 class DataManager{
    + save(): void
    + load(): void
 }

 class TodoList{
    + dataArray: TodoArray
 }

 Todo <-- Add
 Todo <-- Delete
 Todo <-- ChangeCompleted

Add <-- TodoList
Delete <-- TodoList
ChangeCompleted <-- TodoList
Show <-- TodoList
DataManager <-- TodoList
@enduml
